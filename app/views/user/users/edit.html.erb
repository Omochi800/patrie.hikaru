<%= form_with model: @user,url: user_path,method: :patch do |f| %>
<div>
  <%= link_to 'キャンセル',user_path(@user) %>

  <span>プロフィールを編集</span>

  <%= f.submit '完了' %>
</div>
<%= image_tag @user.get_profile_image(80,80),class:'rounded-circle' %>
<ul>
  <li>
    <%= f.text_field :user_name %>
  </li>
  <li>
   <%= f.text_area :intoduction %>
  </li>

</ul>
<% end %>

